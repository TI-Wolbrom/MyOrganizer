                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="BMI" editable="false" layoutX="169.0" layoutY="222.0" prefHeight="25.0" prefWidth="300.0" AnchorPane.leftAnchor="169.0" AnchorPane.rightAnchor="51.0" />
            <Button fx:id="oblicz" layoutX="364.0" layoutY="160.0" mnemonicParsing="false" onAction="#oblicz" prefHeight="25.0" prefWidth="106.0" text="Oblicz" AnchorPane.rightAnchor="50.0" />
